{
  "applications": {
    "gecko": {
      "strict_min_version": "58.0",
      "id": "tabs2text@github.deniszanin"
    }
  },

  "browser_specific_settings": {
    "gecko": {
      "strict_min_version": "58.0"
    }
  },

  "background": {
      "scripts": ["background.js"]
  },

  "browser_action": {
    "browser_style": true,
    "default_icon": {
      "16": "icons/t2t-ico.svg",
      "32": "icons/t2t-ico.svg",
      "48": "icons/t2t-ico.svg",
      "96": "icons/t2t-ico.svg"
    },
    "default_popup": "tabs2text-settings.html",
    "default_title": "Tabs2Text"
  },

  "commands": {
    "tabs2text-export": {
      "suggested_key": {
        "default": "Ctrl+Shift+Y",
        "linux": "Ctrl+Shift+U",
        "mac": "Command+Shift+U",
        "windows": "Ctrl+Shift+Y"
      },
      "description": "Export current browser tabs to file."
    }
  },

  "description": "Export all your opened tabs to a CSV, JSON or Markdown file.",

  "homepage_url": "https://github.com/deniszanin/tabs2text",

  "icons": {
      "16": "icons/t2t-ico.svg",
      "32": "icons/t2t-ico.svg",
      "48": "icons/t2t-ico.svg",
      "96": "icons/t2t-ico.svg"
  },

  "manifest_version": 2,

  "name": "Tabs2Text",

  "options_ui": {
    "page": "tabs2text-settings.html"
  },

  "permissions": [
    "tabs",
    "downloads"
  ],
  
  "version": "2020.4.24"
}
